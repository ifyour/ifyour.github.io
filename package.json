{
  "name": "ifyour-blog",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/hexo s -o",
    "dev": "node_modules/.bin/nodemon -e jade,md,scss,yml --ignore public/ -L --exec 'node_modules/.bin/hexo s && node_modules/.bin/hexo g'",
    "build": "node_modules/.bin/hexo clean && node_modules/.bin/hexo g",
    "publish": "node_modules/.bin/hexo d"
  },
  "hexo": {
    "version": "3.7.1"
  },
  "dependencies": {
    "hexo": "^3.7.1",
    "hexo-algolia": "^1.3.1",
    "hexo-browsersync": "^0.2.0",
    "hexo-deployer-git": "^0.2.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.2",
    "hexo-generator-feed": "^1.2.0",
    "hexo-generator-index": "^0.1.2",
    "hexo-generator-sitemap": "^1.1.2",
    "hexo-generator-tag": "^0.1.1",
    "hexo-renderer-ejs": "^0.1.0",
    "hexo-renderer-jade": "^0.4.1",
    "hexo-renderer-marked": "^0.2.4",
    "hexo-renderer-stylus": "^0.3.0",
    "hexo-service-worker": "^1.0.1",
    "hexo-server": "^0.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
